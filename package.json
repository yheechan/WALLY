{
  "name": "wally",
  "displayName": "wally",
  "description": "We cAn LocaLize Your faulty lines",
  "version": "0.0.1",
  "icon": "./assets/icon/wally_lens.png",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:wally.run",
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wally.run",
        "title": "WALLY"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "wally.run",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "when": "editorFocus",
          "command": "wally.run",
          "group": "navigation",
          "icon": {
            "dark": "./assets/icon/wally.png",
            "light": "./assets/icon/wally.png"
          }
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}